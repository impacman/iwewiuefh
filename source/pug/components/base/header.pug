mixin header(headerMods, currentUrl)
  -
    const headerLinks = [
      {
        href: "#",
        text: "home",
        block: "home"
      },
      {
        href: "#",
        text: "products",
        block: "products"
      },
      {
        href: "#",
        text: "payouts",
        block: "payouts"
      },
      {
        href: "#",
        text: "why us",
        block: "why"
      },
    ]

  header.header(class=headerMods data-header)
    .header__wrapper
      a.logo(href="#")
        img(src="img/svg/logo.svg", alt="Lodo DatingGroup") 
      button.open-menu(data-menu-open) 
        span 
        //- svg(width="31" height="19" viewBox="0 0 31 19" fill="none" xmlns="http://www.w3.org/2000/svg")
        //-   rect(x="0.804688" y="17.1172" width="29.4537" height="1.88467" rx="0.942333" fill="white")
        //-   rect(x="0.804688" y="8.55859" width="29.4537" height="1.88467" rx="0.942333" fill="white")
        //-   rect(x="0.804688" width="29.4537" height="1.88467" rx="0.942333" fill="white")
      nav.main-nav(data-menu-nav)
        ul.main-nav__list(data-animation)
          each link in headerLinks
            li.main-nav__item
                a.main-nav__link(href=link.href data-link=link.block)=link.text
        a.link.link_pink.link_right.link_header(href="https://docs.google.com/forms/d/e/1FAIpQLSdxavG1J88MJNsgx5W9-VNk5QZRPuwnuSUyez7N73GYifXn0Q/viewform" data-animation) 
          span.link__text Apply for program 
