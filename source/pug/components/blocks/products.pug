mixin products()
    section.products(data-block="products")
        .products__wrapper
            h2.title.title_blue.title_min.title_center(data-animation) Our products    
            .products-bullet.swiper(data-products-bullet) 
                .products-bullet__wrapper.swiper-wrapper
                    .products-bullet__slide.swiper-slide
                        .products-bullet__text(data-animation) Dating.—Åom
                    .products-bullet__slide.swiper-slide
                        .products-bullet__text(data-animation) AnastasiaDates
                    .products-bullet__slide.swiper-slide
                        .products-bullet__text(data-animation) AsianDate
                    .products-bullet__slide.swiper-slide
                        .products-bullet__text(data-animation) YourTravelMates
                    .products-bullet__slide.swiper-slide
                        .products-bullet__text(data-animation) ArabianDate
                    .products-bullet__slide.swiper-slide
                        .products-bullet__text(data-animation) Amolatina
                    .products-bullet__slide.swiper-slide
                        .products-bullet__text(data-animation) Eurodate
                    .products-bullet__slide.swiper-slide
                        .products-bullet__text(data-animation) ChinaLove
                    .products-bullet__slide.swiper-slide
                        .products-bullet__text(data-animation) RussianBrides
                    .products-bullet__slide.swiper-slide
                        .products-bullet__text(data-animation) DateMyAge

            .products-slider.swiper(data-products-slider data-animation) 
                .products-slider__wrapper.swiper-wrapper
                    .products-slider__slide.swiper-slide
                        .products-slider__wrap-img
                            img.products-slider__img.swiper-lazy(data-src="img/products/1.jpg", alt="")
                            .swiper-lazy-preloader
                    .products-slider__slide.swiper-slide
                        .products-slider__wrap-img
                            img.products-slider__img.swiper-lazy(data-src="img/products/2.jpg", alt="")
